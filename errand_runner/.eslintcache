dleChange for our paymentsAccepted based on paymentOptions above\n  const handleChange = (po, e) => {\n    const { checked } = e.target;\n    if (checked) {\n      setPaymentsAccepted([...paymentsAccepted, po]);\n    } else {\n      setPaymentsAccepted(paymentsAccepted.filter((p) => p !== po));\n    }\n  }\n\n  const handleSubmit = async (e) => {\n    e.preventDefault()\n    const fields = {\n      errand,\n      organization, \n      notes, \n      paymentsAccepted,\n      openingHours, \n      closingHours,\n    }\n    const resp = await axios.post(baseURL, { fields }, config)\n    props.setToggle((prev) => !prev)\n    history.push(\"/\")\n  }\n\n  return (\n    <form onSubmit={handleSubmit}>\n      <h4>Make a new errand!</h4>\n      <label htmlFor=\"errand\">Errand</label>\n      <input\n        name=\"errand\"\n        type=\"text\"\n        value={errand}\n        onChange={(e) => setErrand(e.target.value)}\n      />\n      <label htmlFor=\"organization\">Organization</label>\n      <input\n        name=\"organization\"\n        type=\"text\"\n        onChange={(e) => setOrganization(e.target.value)}\n        value={organization}\n      />\n      <label htmlFor=\"notes\">Notes</label>\n      <input\n        name=\"notes\"\n        type=\"text\"\n        value={notes}\n        onChange={(e) => setNotes(e.target.value)}\n      />\n      {/* Mapping through paymentOptions below to make individual checkboxes for each option */}\n      <div className=\"paymentOptions\">\n      {paymentOptions.map((po) => (\n        <>\n          <label htmlFor={po}>{po}</label> \n          <input name={po} type=\"checkbox\" checked={paymentsAccepted.includes(po)} onChange={(e) => handleChange(po, e)} />\n        </>\n      ))}\n      </div>\n      <label htmlFor=\"openingHours\">Opening Hours</label>\n      <input\n        name=\"openingHours\"\n        type=\"number\"\n        value={openingHours}\n        onChange={(e) => setOpeningHours(e.target.valueAsNumber)}\n      />\n      <label htmlFor=\"closingHours\">Closing Hours</label>\n      <input\n        name=\"closingHours\"\n        type=\"number\"\n        value={closingHours}\n        onChange={(e) => setClosingHours(e.target.valueAsNumber)}\n      />\n      <button type=\"submit\">Submit</button>\n    </form>\n  );\n}\n\nexport default Form;","/Users/love/Desktop/General_Assembly/Project/GA-Project-2/errand_runner/src/components/Home.jsx",[],"/Users/love/Desktop/General_Assembly/Project/GA-Project-2/errand_runner/src/components/Show.jsx",[],"/Users/love/Desktop/General_Assembly/Project/GA-Project-2/errand_runner/src/components/Footer.jsx",["73"],"import {Link} from \"react-router-dom\";\n\nfunction Footer() {\n  return (\n    <div className=\"footer\">\n      {/* <Link to=\"\"><img src=\"#\" alt=\"github link\" target=\"_blank\"/></Link> */}\n      <a href=\"https://github.com/er812\" target=\"_blank\" rel=\"noopener noreferrer\">\n          <img src=\"https://github.githubassets.com/images/modules/logos_page/Octocat.png\" alt=\"GitHub\" />\n        </a>\n    </div>\n  )\n}\n\nexport default Footer\n\n//add github icon and link to github\n//mkae it a different color","/Users/love/Desktop/General_Assembly/Project/GA-Project-2/GA-Project-2/errand_runner/src/index.js",[],"/Users/love/Desktop/General_Assembly/Project/GA-Project-2/GA-Project-2/errand_runner/src/App.js",[],"/Users/love/Desktop/General_Assembly/Project/GA-Project-2/GA-Project-2/errand_runner/src/components/Show.jsx",[],"/Users/love/Desktop/General_Assembly/Project/GA-Project-2/GA-Project-2/errand_runner/src/components/Footer.jsx",["74"],"/Users/love/Desktop/General_Assembly/Project/GA-Project-2/GA-Project-2/errand_runner/src/components/Nav.jsx",[],"/Users/love/Desktop/General_Assembly/Project/GA-Project-2/GA-Project-2/errand_runner/src/components/Form.jsx",["75"],"/Users/love/Desktop/General_Assembly/Project/GA-Project-2/GA-Project-2/errand_runner/src/components/Home.jsx",[],"/Users/love/Desktop/General_Assembly/Project/GA-Pro